# WalletConnect Project ID (Required)
# Get your project ID from https://cloud.walletconnect.com/
# This is required for WalletConnect to work properly
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Smart Contract Addresses (Base Sepolia)
# Factory contract address yang sudah di-deploy
NEXT_PUBLIC_FACTORY_ADDRESS=0x_your_factory_address_here

# Token address (MockUSDC atau token lain yang digunakan)
NEXT_PUBLIC_TOKEN_ADDRESS=0x_your_token_address_here

# X (Twitter) OAuth 2.0 Configuration
# Get your OAuth 2.0 credentials from https://developer.twitter.com/en/portal/projects-and-apps
# Required for user authentication via OAuth 2.0
NEXT_PUBLIC_X_CLIENT_ID=your_oauth2_client_id_here
X_CLIENT_SECRET=your_oauth2_client_secret_here
NEXT_PUBLIC_X_CALLBACK_URL=http://localhost:3000/api/auth/twitter/callback
